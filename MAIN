print("KAHCustom loaded.")

game.Players.LocalPlayer.Chatted:connect(function(msg)

	if string.sub(msg, 1, 5) == "swkit" then

		local plrname = string.sub(msg, 7)
		game:GetService'Players':Chat(("gear "..plrname.." 108149175"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 101191388"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 158069180"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 81154592"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 68603324"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 77443704"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 83704165"))
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 77443461"))-- sword of dark
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 77443491"))-- sword of light
	wait(.001)
		game:GetService'Players':Chat(("gear "..plrname.." 37816777"))--ghostwalker
	end
end)


