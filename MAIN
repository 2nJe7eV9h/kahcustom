game.Players.LocalPlayer.Chatted:Connect(function(msg)
	if string.sub(msg, 1, 5) == "clean" then
		game:GetService'Players':Chat(("clr"))
	wait(.001)
		game:GetService'Players':Chat(("fix"))
	wait(.001)
		game:GetService'Players':Chat(("respawn all"))
	end
end)

