newButton(
    "Copy Remote",
    "Click to copy the path of the remote",
    function(button)
        local orText = "Click to copy the path of the remote"
        syn.write_clipboard(typeToString(selected.Remote))
        button.Text = "Copied!"
        wait(3)
        button.Text = orText
    end
)

